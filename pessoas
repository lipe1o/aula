public class pessoas{
    private String nome;
    private int idade;

    public void setNome(String nome1){
        this.nome=nome1;
    }
    public String getNome(){
        return this.nome;
    }
    public void setIdade(int idade1){
        this.idade=idade1;
    }
    public int getIdade(){
        return this.idade;
    }
}
